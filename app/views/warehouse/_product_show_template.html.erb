<script type="text/template" class="template">
  <table class="table" id="{{= rc.id }}">
    <thead>
      <tr>
        <th>Image</th>
        <th>Codigo unico</th>
        <th>Descripcion</th>
      </tr>
    </thead>
    <tbody>
      <tr>
        <td class='image'><a href="{{= rc.link }}" ><img src="{{= rc.image }}" alt="{{= rc.description }}" /></a></td>
        <td class='unique_code'>{{= rc.unique_code }}</td>
        <td class='description'>{{= rc.description }}</td>
      </tr>
    </tbody>
  </table>
</script>
