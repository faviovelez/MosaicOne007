<td>
  <a href="javascript:;" class="close-icon" id="close_icon_">
  </a>
</td>
<td class="space-preview hidden product_id">
  <%= select_tag 'product_id[]', options_for_select(@products_ids), class: 'form-control select-margin product_id', readonly: true %>
</td>
<td class="space-preview sat_key"> <%= select_tag 'sat_key[]', options_for_select(@products_sat_keys), class: 'form-control select-margin sat_key', readonly: true %> </td>
<td class="space-preview unique_code" style="width: 5%"> <%= text_field_tag 'unique_code[]', nil, {class: 'select-product form-control unique_code'} %> </td>
<td class="space-preview quantity"> <%= text_field_tag 'quantity[]', nil, {placeholder: '100', class: 'form-control center-input quantity'} %> </td>
<td class="space-preview sat_unit_key"> <%= select_tag 'sat_unit_key[]', options_for_select(@products_sat_unit_keys), class: 'form-control select-margin sat_unit_key', readonly: true %> </td>
<td class="space-preview sat_unit_description"> <%= select_tag 'sat_unit_description[]', options_for_select(@products_units), class: 'form-control select-margin sat_unit_description', readonly: true %> </td>
<td class="space-preview product_description_select hidden"> <%= select_tag 'product_description_select[]', options_for_select(@products_description), class: 'form-control select-margin product_description' %> </td>
<td class="space-preview product_description"> <%= text_field_tag 'product_description[]', nil, {class: 'form-control select-margin product_description'} %> </td>
<td class="space-preview unit_value_hidden"> <%= text_field_tag 'unit_value_hidden[]', nil, {placeholder: '100', class: 'form-control center-input unit_value_hidden'} %> </td>
<td class="space-preview unit_value hidden"> <%= select_tag 'unit_value[]', options_for_select(@products_prices), class: 'form-control select-margin unit_value', readonly: true %> </td>
<td class="space-preview discount"> <%= text_field_tag 'discount[]', nil, {placeholder: '$ 0.00', class: 'form-control center-input discount'} %> </td>
<td class="space-preview taxes"> <%= text_field_tag 'taxes[]', nil, {placeholder: '$ 0.00', class: 'form-control center-input', readonly: true} %> </td>
<td class="space-preview subtotal"> <%= text_field_tag 'subtotal[]', nil, {class: 'form-control center-input', readonly: true} %> </td>
